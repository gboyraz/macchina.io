            "\t\t}",
            "\t\tvoid stop(Poco::OSP::BundleContext::Ptr pContext){",
            "\t\t}",
            "};",
            "}",
            "POCO_BEGIN_MANIFEST(Poco::OSP::BundleActivator)",
            "POCO_EXPORT_CLASS(Avioos::BundleActivator)",
            "POCO_END_MANIFEST"
        ]
    }
}